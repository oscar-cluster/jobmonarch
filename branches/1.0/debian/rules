#!/usr/bin/make -f

%:
	dh $@ --with python2

override_dh_auto_install:
	$(MAKE) DESTDIR=$(CURDIR)/debian/tmp PREFIX=/usr GANGLIA_ROOT=/usr/share/ganglia-webfrontend GANGLIA_USER=ganglia HTTPD_USER=www-data JOBARCHIVE_RRDS=/var/lib/jobarchive install
	dh_install --autodest --sourcedir=$(CURDIR)/debian/tmp
